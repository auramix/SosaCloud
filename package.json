{
  "name": "sidebar-views",
  "version": "1.0.0",
  "description": "FEC project",
  "main": "./server/index.js",
  "scripts": {
    "test": "jest --coverage --detectOpenHandles",
    "update_tests": "jest --updateSnapshot",
    "server": "nodemon ./server/server.js",
    "react-dev": "webpack -d --watch",
    "fakeData": "node db/fakeData.js",
    "seed": "mysql -u root -p < db/schema.sql",
    "dev": "concurrently \"npm run seed\" \"npm run fakeData\" \"npm run server\" \"npm run react-dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sosacloud/sidebar-views.git"
  },
  "author": "Scott Josephson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sosacloud/sidebar-views/issues"
  },
  "homepage": "https://github.com/sosacloud/sidebar-views#readme",
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "./test_coverage",
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme"
  },
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "lodash": "^4.17.19",
    "mysql": "^2.17.1",
    "nodemon": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "jest-styled-components": "^6.3.3",
    "react-test-renderer": "^16.8.6",
    "supertest": "^4.0.2",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  }
}